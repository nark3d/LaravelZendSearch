{
    "name": "best-served-cold/laravel-zendsearch",
    "description": "ZendSearch implementation for Laravel - Local file based search engine",
    "keywords": ["laravel", "lucene", "zendsearch", "eloquent", "search", "local search engine", "file based search engine"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Lewis",
            "email": "adam.lewis@bestservedcold.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "zendframework/zendsearch": "^2.0@RC",
        "illuminate/support": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "orchestra/testbench": "^3.2",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "BestServedCold\\LaravelZendSearch\\": "src/BestServedCold/LaravelZendSearch"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "BestServedCold\\LaravelZendSearch\\": "tests"
        }
    }
}
